doctype html
html(lang="ja")
  head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0")
    script(src=js_path + "/lib/html5shiv.js")
    link(rel="stylesheet", href=css_path + "//*[= name ]*/.css")
